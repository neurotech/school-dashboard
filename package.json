{
  "name": "app",
  "productName": "Picnic",
  "version": "0.0.1",
  "description": "Dashboard",
  "license": "MIT",
  "repository": "https://github.com/neurotech/picnic",
  "author": {
    "name": "Tim Douglas",
    "email": "neurotech@gmail.com",
    "url": "https://github.com/neurotech"
  },
  "scripts": {
    "test": "xo",
    "build:css": "node src/palette/index.js && node-sass src/scss/all.scss app/css/index.css",
    "vendor": "cp node_modules/normalize.css/normalize.css app/css/normalize.css && cp node_modules/lemonade-grid/css/lemonade.css app/vendor/lemonade.css",
    "prestart": "npm run build:css && npm run vendor",
    "start": "electron .",
    "prebuild": "npm run build:css && npm run vendor",
    "build": "npm run build:osx && npm run build:linux && npm run build:windows",
    "build:osx": "electron-packager . Picnic --overwrite --asar --out=dist --ignore='^src$' --prune --platform=darwin --arch=x64 --app-version=$npm_package_version",
    "build:linux": "electron-packager . Picnic --overwrite --out=dist --ignore='^src$' --prune --platform=linux --arch=x64 --app-version=$npm_package_version",
    "build:windows": "electron-packager . Picnic --overwrite --asar --out=dist --ignore='^src$' --prune --platform=win32 --arch=ia32 --app-version=$npm_package_version"
  },
  "keywords": [
    "electron"
  ],
  "dependencies": {
    "chart.js": "2.1.5",
    "electron-config": "0.1.1",
    "electron-debug": "^1.0.0",
    "electron-packager": "7.0.3",
    "got": "6.3.0",
    "moment": "2.13.0",
    "normalize.css": "4.1.1",
    "pug": "2.0.0-beta2",
    "velocity-animate": "1.2.3",
    "vue": "1.0.24"
  },
  "devDependencies": {
    "devtron": "^1.1.0",
    "electron-packager": "^7.0.0",
    "electron-prebuilt": "^1.2.2",
    "semistandard": "8.0.0",
    "xo": "^0.15.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  }
}
