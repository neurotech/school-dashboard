{
  "name": "app",
  "productName": "Picnic",
  "version": "0.0.1",
  "description": "Dashboard",
  "license": "MIT",
  "repository": "https://github.com/neurotech/picnic",
  "author": {
    "name": "Tim Douglas",
    "email": "neurotech@gmail.com",
    "url": "https://github.com/neurotech"
  },
  "scripts": {
    "test": "xo",
    "build:css": "node-sass src/scss/all.scss app/css/index.css",
    "prestart": "npm run build:css",
    "start": "electron .",
    "prebuild": "npm run build:css",
    "build": "npm run build:osx && npm run build:linux && npm run build:windows",
    "build:osx": "electron-packager . Picnic --overwrite --asar --out=dist --prune --platform=darwin --arch=x64 --app-version=$npm_package_version",
    "build:linux": "electron-packager . Picnic --overwrite --out=dist --prune --platform=linux --arch=x64 --app-version=$npm_package_version",
    "build:windows": "electron-packager . Picnic --overwrite --asar --out=dist --prune --platform=win32 --arch=x64 --app-version=$npm_package_version"
  },
  "keywords": [
    "electron"
  ],
  "dependencies": {
    "chart.js": "2.1.5",
    "electron-config": "0.1.1",
    "electron-debug": "^1.0.0",
    "electron-packager": "7.0.3",
    "got": "6.3.0",
    "jquery": "3.0.0",
    "moment": "2.13.0",
    "page": "1.7.1",
    "pug": "2.0.0-beta2",
    "vue": "1.0.24"
  },
  "devDependencies": {
    "devtron": "^1.1.0",
    "electron-packager": "^7.0.0",
    "electron-prebuilt": "^1.2.2",
    "semistandard": "8.0.0",
    "xo": "^0.15.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  }
}
